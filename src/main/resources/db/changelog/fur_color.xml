<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
                   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                   xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
                   http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.8.xsd">

        <changeSet id="20230427-01" author="John Doe">
            <createTable tableName="fur_color">
                <column name="id" type="BIGINT" autoIncrement="true">
                    <constraints primaryKey="true"/>
                </column>
                <column name="name" type="VARCHAR(255)"/>
            </createTable>
        </changeSet>

        <changeSet id="20230427-02" author="John Doe">
            <insert tableName="fur_color">
                <column name="name" value="Black"/>
            </insert>
            <insert tableName="fur_color">
                <column name="name" value="White"/>
            </insert>
            <insert tableName="fur_color">
                <column name="name" value="Brown"/>
            </insert>
            <insert tableName="fur_color">
                <column name="name" value="Yellow"/>
            </insert>
            <insert tableName="fur_color">
                <column name="name" value="Blue"/>
            </insert>
        </changeSet>
</databaseChangeLog>

